    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "google-maps-react": "^1.1.2",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "pg-promise": "^8.2.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "underscore": "^1.8.3"
