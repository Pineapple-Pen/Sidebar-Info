{
  "name": "map-side-bar",
  "version": "1.0.0",
  "description": "info sidebar with a map for WeGot restaurant review website",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bamboo-connection/map-side-bar.git"
